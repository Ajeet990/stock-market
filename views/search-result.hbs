<h1>Search result for "<b>{{item}}</b>"</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Available stocks</th>
            <th scope="col">Cost per Stock</th>


        </tr>
    </thead>
    <tbody>
        {{#each rows}}




        <tr>


            <script>document.write("<td>" + ({{@index}} + 1) +"</td>")</script>
            <td>{{this.name}}</td>
            <td>{{this.available_stock}} only</td>
            <td>Rs. {{this.per_s_price}} /-</td>


        </tr>



        {{/each}}





    </tbody>
</table>